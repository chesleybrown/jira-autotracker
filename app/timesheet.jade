style.
	body {
		margin: 0;
		font: 16px "Helvetica Neue", Helvetica;
	}
	nav {
		border-bottom: 1px solid #2e3d54;
		margin-top: 0;
		background: #205081;
	}
	nav h1 {
		font-weight: normal;
		color: #fff;
		margin: 0;
		padding: 5px 10px;
	}
	table {
		padding: 50px;
		width: 100%;
	}
	table th {
		margin-top: 10px;
		text-align: left;
	}
	table td {
		font-size: 12px;
		border-bottom: 1px solid #999;
	}
	table td span {
		font-size: 10px;
	}

nav
	h1 Jira Autotracker

table
	each developer, name in developers
		tr
			th
			each date in dates
				th= date
		tr
			th= name
		each issue, issueKey in developer
			tr
				td #{issueKey} 
					span= issue.issue.fields.summary
				each hours in issue.date
					td #{hours} hours
